<refentry xmlns="http://docbook.org/ns/docbook" xml:id="imagick.getimagelength">
 <refnamediv>
  <refname>Imagick::getImageLength</refname>
  <refpurpose>Retorna o comprimento da imagem em bytes</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>Imagick::getImageLength</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Retorna o comprimento da imagem em bytes.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna um inteiro contendo o tamanho atual da imagem.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Usando <function>Imagick::getImageLength</function></title>
    <para>
     Obtendo o comprimento da imagem em bytes
    </para>
    <programlisting role="php">
<![CDATA[
<?php
$image = new Imagick('test.jpg');
echo $image->getImageLength() . ' bytes';
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>

</refentry>

