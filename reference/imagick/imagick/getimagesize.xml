<refentry xmlns="http://docbook.org/ns/docbook" xml:id="imagick.getimagesize">
 <refnamediv>
  <refname>Imagick::getImageSize</refname>
  <refpurpose>Retorna o comprimento da imagem em bytes</refpurpose>
 </refnamediv>

 <refsynopsisdiv>
   &imagick.deprecated.function-3-4-4;
 </refsynopsisdiv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>Imagick::getImageSize</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Retorna o comprimento da imagem em bytes.
   Função preterida em favor de <link linkend="imagick.getimagelength">Imagick::getImageLength()</link>
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna um int contendo o comprimento atual da imagem.
  </para>
 </refsect1>

</refentry>

